@prefix lv2:     <http://lv2plug.in/ns/lv2core#> .
@prefix guiext:  <http://ll-plugins.nongnu.org/lv2/ext/gui#> .
@prefix drobins: <http://drobilla.net/lv2_plugins/dev/> .

drobins:bang
